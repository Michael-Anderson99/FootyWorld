<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.footyworld">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StatsSection"></activity>
        <activity android:name=".User.TeamDisplay" />
        <activity android:name=".AlgorithmActivity" />
        <activity android:name=".pickTeams" />
        <activity android:name=".AppDatabase" />
        <activity android:name=".User.User" />
        <activity android:name=".User.UserRepository" />
        <activity android:name=".User.UserDao" />
        <activity android:name=".CreateSquad" />
        <activity android:name=".CreateProfile" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>